# API代理服务密钥
# 选择一个或多个API服务填写密钥

# ScrapingAPI (https://www.scrapingapi.com/)
SCRAPINGAPI_KEY=your_key_here

# ScrapingDog (https://www.scrapingdog.com/)
SCRAPINGDOG_KEY=your_key_here

# ScrapeStack (https://scrapestack.com/)
SCRAPESTACK_KEY=your_key_here

# Bright Data (https://brightdata.com/)
BRIGHTDATA_USERNAME=your_username
BRIGHTDATA_PASSWORD=your_password

# ZenRows (https://www.zenrows.com/)
ZENROWS_API_KEY=your_key_here

# ScraperAPI (https://www.scraperapi.com/)
SCRAPERAPI_KEY=your_key_here

# AI API密钥
OPENAI_API_KEY=your_openai_key
DEEPSEEK_API_KEY=your_deepseek_key

# 应用配置
# 使用的API代理服务 (scrapingapi, scrapingdog, scrapestack, scraperapi, zenrows)
PREFERRED_API_PROXY=scrapingapi

# 请求配置
MIN_DELAY=0.5
MAX_DELAY=2.0
MAX_RETRIES=3
TIMEOUT=30

# 调试模式
DEBUG=False 